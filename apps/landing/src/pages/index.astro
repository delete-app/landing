                <a href="https://twitter.com/trydeleteapp" target="_blank" rel="noopener">X</a>
            </div>
        </div>
    </footer>
    <script>
        document.getElementById('f').onsubmit=e=>{
            e.preventDefault();
            const email=encodeURIComponent(document.querySelector('[name=email]').value);
            const city=encodeURIComponent(document.querySelector('[name=city]').value);
            window.location.href='https://tally.so/r/rjD9ZL?prefill_email='+email+'&prefill_city='+city;
        }
    </script>
