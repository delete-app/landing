        h1{font-size:clamp(1.75rem,5vw,2.5rem);font-weight:600;line-height:1.15;letter-spacing:-.02em;margin-bottom:.75rem}
        .sub{font-size:clamp(1.1rem,2.5vw,1.25rem);color:#888;margin-bottom:2.5rem}
        form{display:flex;flex-direction:column;gap:.75rem}
        input,select{width:100%;padding:1rem;font-size:1rem;font-family:inherit;background:#141414;border:1px solid #222;border-radius:6px;color:#fafafa;appearance:none}
        input::placeholder{color:#666}
        select{color:#666;cursor:pointer}
        select:valid{color:#fafafa}
        input:focus,select:focus{outline:none;border-color:#e67e22}
        button{padding:1rem 1.5rem;font-size:1rem;font-weight:500;font-family:inherit;background:#e67e22;color:#0a0a0a;border:none;border-radius:6px;cursor:pointer}
