        .note{font-size:.875rem;color:#555;margin-top:.75rem}
        .escape{font-size:.875rem;color:#666;margin-top:1.25rem}
        .escape a{color:#888;text-decoration:none;transition:color .15s}
        .escape a:hover{color:#e67e22}
        .pts{padding:2rem 0 4rem}
        .pts p{color:#666;margin-bottom:.75rem;padding-left:1.25rem;position:relative}
        .pts p::before{content:'';position:absolute;left:0;top:.55rem;width:5px;height:5px;background:#e67e22;border-radius:50%}
        .why{padding:2rem 0 3rem}
        .why-link{display:flex;flex-direction:column;gap:.5rem;padding:1.5rem;background:linear-gradient(135deg,rgba(230,126,34,.08) 0%,rgba(230,126,34,.02) 100%);border:1px solid rgba(230,126,34,.2);border-radius:12px;text-decoration:none;transition:all .2s ease}
        .why-link:hover{border-color:rgba(230,126,34,.5);transform:translateY(-2px)}
        .why-label{color:#888;font-size:.95rem}
        .why-cta{color:#e67e22;font-weight:500;font-size:1.1rem;display:flex;align-items:center;gap:.5rem}
        .why-cta .arrow{transition:transform .2s ease}
        .why-link:hover .arrow{transform:translateX(4px)}
        .trust{padding:2rem 0 3rem}
        .trust-box{padding:2rem;background:#0f0f0f;border:1px solid #1a1a1a;border-radius:12px}
        .trust-headline{font-size:1.25rem;font-weight:500;color:#fafafa;margin-bottom:.5rem;line-height:1.3}
        .trust-headline em{font-style:normal;color:#e67e22}
        .trust-sub{color:#666;margin-bottom:1.5rem;font-size:1rem}
        .trust-link{display:inline-flex;align-items:center;gap:.5rem;color:#e67e22;text-decoration:none;font-weight:500;font-size:1rem;transition:gap .2s ease}
        .trust-link:hover{gap:.75rem}
